// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Test {
  id    String @id @default(uuid())
  title String

}

model Artworks {
  artworkId String @id @default(uuid())
  blurb String

}

model Artist {
  artistId String @id @default(uuid())
  // artworks Artworks?   @relation(fields: [artistId], references: [artworkId])
}
